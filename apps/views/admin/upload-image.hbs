    <script>
        var id = $('#idConstruct').text();
        $(function () {
            $('#fuMain').fileinput({
                theme: 'fa',
                dropZoneEnabled: false,
                allowedFileExtensions: ['png', 'jpg', 'gif'],
                uploadUrl: '/admin/construct/' + id + '/upload',
                uploadAsync: false,
            });
        })
        
        function deleteFrm(nameFrm) {
            var frm = $(nameFrm);
            var act = frm.attr('action') + '?action=' + id;
            frm.attr('action', act);
            console.log(act);
            var rs = window.confirm("Are you sure?");
            if (rs) {
                frm.submit();
            }
        }

