<!-- partial -->
<div class="card">
    <div class="card-body">
        <h4 class="card-title mb-5">Bảng dữ liệu khách hàng
        </h4>
        <div class="row">
            <div class="col-12">
                <div class="table-responsive">
                    <table id="order-listing" class="table">
                        <thead>
                            <tr>
                                <th width="5%">ID</th>
                                <th width="15%">Tên</th>
                                <th width="15%">Email</th>
                                <th width="15%">Phone</th>
                                <th width="30%">Tin nhắn</th>
                                <th width="20%">Ngày tạo</th>
                            </tr>
                        </thead>
                        <tbody>
                            {{#each clients}}
                            <tr>
                                <td>{{id}}</td>
                                <td>{{name}}</td>
                                <td>{{email}}</td>
                                <td>{{phone}}</td>
                                <td>{{message}}</td>
                                <td>{{moment created_at format="MM/DD h:mm"}}</td>
                            </tr>
                            {{/each}}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>